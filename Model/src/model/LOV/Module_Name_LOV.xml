<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="Module_Name_LOV"
  Version="12.2.1.11.57"
  InheritPersonalization="merge"
  SelectList="TblModule.AST, 
       TblModule.MODULE_ID, 
       TblModule.NAME"
  FromList="TBL_MODULE TblModule"
  Where="AST='Y'"
  BindingStyle="OracleName"
  CustomQuery="false"
  PageIterMode="Full"
  OrderBy="TblModule.NAME">
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="model.LOV.Module_Name_LOV_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <EntityUsage
    Name="TblModule"
    Entity="model.EO.TblModule"/>
  <ViewAttribute
    Name="Ast"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="Ast"
    EntityUsage="TblModule"
    AliasName="AST"/>
  <ViewAttribute
    Name="ModuleId"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="ModuleId"
    EntityUsage="TblModule"
    AliasName="MODULE_ID"/>
  <ViewAttribute
    Name="Name"
    IsUnique="true"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="Name"
    EntityUsage="TblModule"
    AliasName="NAME"/>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
